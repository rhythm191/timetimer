{
  "manifest_version": 3,
  "name": "timetimer",
  "version": "0.1",
  "description": "time timer on chrome",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["images/timer-scale.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}
